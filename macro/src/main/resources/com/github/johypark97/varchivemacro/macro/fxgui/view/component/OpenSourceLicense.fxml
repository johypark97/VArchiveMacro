<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<fx:root xmlns:fx="http://javafx.com/fxml/1" spacing="10.0" type="HBox" xmlns="http://javafx.com/javafx/21">
  <children>
    <ListView fx:id="libraryListView"/>
    <VBox HBox.hgrow="ALWAYS" spacing="10.0">
      <children>
        <TextArea VBox.vgrow="ALWAYS" editable="false" fx:id="libraryLicenseTextArea"/>
        <HBox alignment="CENTER_LEFT" spacing="10.0">
          <children>
            <Label text="URL"/>
            <TextField HBox.hgrow="ALWAYS" editable="false" fx:id="libraryUrlTextField"/>
          </children>
        </HBox>
      </children>
    </VBox>
  </children>
  <padding>
    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
  </padding>
</fx:root>
