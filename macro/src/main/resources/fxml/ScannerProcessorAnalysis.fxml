<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressIndicator?>
<?import javafx.scene.layout.VBox?>
<fx:root xmlns:fx="http://javafx.com/fxml/1" alignment="CENTER" spacing="32.0" type="VBox" xmlns="http://javafx.com/javafx/21.0.6">
  <children>
    <VBox alignment="TOP_CENTER">
      <children>
        <ProgressIndicator progress="0.0" scaleX="2.0" scaleY="2.0" fx:id="progressIndicator"/>
        <Label text="Text" fx:id="progressLabel"/>
      </children>
    </VBox>
    <Label text="Text" fx:id="messageLabel"/>
    <Button text="Button" fx:id="functionButton"/>
  </children>
</fx:root>
