<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<fx:root xmlns:fx="http://javafx.com/fxml/1" spacing="10.0" type="VBox" xmlns="http://javafx.com/javafx/21">
  <children>
    <ScrollPane VBox.vgrow="ALWAYS" fitToHeight="true" fitToWidth="true" vbarPolicy="ALWAYS" fx:id="scrollPane">
      <content>
        <VBox spacing="10.0" fx:id="contentBox">
          <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
          </padding>
        </VBox>
      </content>
    </ScrollPane>
    <HBox alignment="TOP_RIGHT">
      <children>
        <Button text="%home.updateCheck.checkAgainButton" fx:id="checkAgainButton"/>
      </children>
    </HBox>
  </children>
  <padding>
    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
  </padding>
</fx:root>
