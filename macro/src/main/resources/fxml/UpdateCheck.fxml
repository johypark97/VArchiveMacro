<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.VBox?>
<fx:root xmlns:fx="http://javafx.com/fxml/1" type="VBox" xmlns="http://javafx.com/javafx/21.0.6">
  <children>
    <ScrollPane VBox.vgrow="ALWAYS" fitToHeight="true" fitToWidth="true" vbarPolicy="ALWAYS" fx:id="scrollPane">
      <content>
        <VBox style="-fx-background-color: white;" spacing="10.0" fx:id="contentBox">
          <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
          </padding>
        </VBox>
      </content>
    </ScrollPane>
    <ButtonBar>
      <buttons>
        <Button text="%updateCheck.button.checkAgain" fx:id="checkAgainButton"/>
        <Button cancelButton="true" text="%updateCheck.button.close" fx:id="closeButton"/>
      </buttons>
      <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
      </padding>
    </ButtonBar>
  </children>
</fx:root>
